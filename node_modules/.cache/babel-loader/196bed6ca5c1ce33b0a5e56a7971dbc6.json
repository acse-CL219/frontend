,CAAA,CAAA,IAAA,QAAA,EACA,QAAA,GAAA,eAAA,CAAA,CAAA,CAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA;AACA;;AAEA,UAAA,MAAA,GAAA;AACA,QAAA,KAAA,EAAA;AAAA,UAAA,IAAA,YAAA,KAAA,UAAA,CAAA,QAAA,EAAA;AAAA,SADA;AAEA,QAAA,OAAA,EAAA,EAFA;AAGA,QAAA,MAAA,EAAA;AAAA,UAAA,IAAA,EAAA,CAAA,SAAA,EAAA,SAAA,EAAA,QAAA,CAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SAHA;AAIA,QAAA,KAAA,EAAA;AACA,UAAA,KAAA,EAAA,QADA;AAEA,UAAA,QAAA,EAAA,EAFA;AAKA,UAAA,SAAA,EAAA,CACA;AAAA,YAAA,IAAA,EAAA,MAAA;AAAA,YAAA,GAAA,EAAA,QAAA;AAAA,YAAA,KAAA,EAAA;AAAA,WADA,EAEA;AAAA,YAAA,IAAA,EAAA,MAAA;AAAA,YAAA,GAAA,EAAA,QAAA;AAAA,YAAA,KAAA,EAAA;AAAA,WAFA,EAGA;AAAA,YAAA,IAAA,EAAA,MAAA;AAAA,YAAA,GAAA,EAAA,QAAA;AAAA,YAAA,KAAA,EAAA;AAAA,WAHA,EAIA;AAAA,YAAA,IAAA,EAAA,MAAA;AAAA,YAAA,GAAA,EAAA,QAAA;AAAA,YAAA,KAAA,EAAA;AAAA,WAJA,EAKA;AAAA,YAAA,IAAA,EAAA,MAAA;AAAA,YAAA,GAAA,EAAA,QAAA;AAAA,YAAA,KAAA,EAAA;AAAA,WALA,EAMA;AAAA,YAAA,IAAA,EAAA,MAAA;AAAA,YAAA,GAAA,EAAA,QAAA;AAAA,YAAA,KAAA,EAAA;AAAA,WANA,EAOA;AAAA,YAAA,IAAA,EAAA,QAAA;AAAA,YAAA,GAAA,EAAA,QAAA;AAAA,YAAA,KAAA,EAAA;AAAA,WAPA;AALA,SAJA;AAmBA,QAAA,MAAA,EAAA,CACA;AAAA,UAAA,IAAA,EAAA,0BAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,IAAA,EAAA;AAFA,SADA;AAnBA,OAAA;AA6BA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,WAAA,KAAA,CAAA,gBAAA,EAAA,YAAA,CAAA,MAAA,EA5CA,CA4CA;AACA,KA5DA;AA8DA,IAAA,eA9DA,6BA8DA;AAAA;;AAEA,WAAA,WAAA,CAAA,MAAA,CAAA,CAAA,EAAA,KAAA,WAAA,CAAA,MAAA;AACA,WAAA,IAAA,CAAA,OAAA,EAAA,aAAA,EAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAFA;AAGA,KApEA;AAoEA;AACA,IAAA,aArEA,2BAqEA;AAAA;;AAEA,WAAA,SAAA,CAAA,MAAA,CAAA,CAAA,EAAA,KAAA,SAAA,CAAA,MAAA;AACA,WAAA,IAAA,CAAA,OAAA,EAAA,WAAA,EAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAFA;AAGA,KA3EA,CA2EA;;AA3EA,GAvCA;AAqHA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,aAAA,KAAA,IAAA,CAAA,KAAA,CAAA,WAAA;AACA,KAHA;AAIA,IAAA,SAJA,uBAIA;AACA,aAAA,KAAA,IAAA,CAAA,KAAA,CAAA,SAAA;AACA;AANA,GArHA;AA6HA,EAAA,IA7HA,kBA6HA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,EADA;AAEA,MAAA,SAAA,EAAA,EAFA;AAGA,MAAA,UAAA,EAAA,KAAA,CAAA,KAHA;AAIA,MAAA,IAAA,EAAA,WAJA;AAOA,MAAA,WAAA,EAAA,CAAA,GAAA,CAPA,CAOA;;AAPA,KAAA;AASA;AAvIA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- 第一行 -->\n    <el-row :gutter=\"40\">\n      <!-- 个人的基本情况卡片 -->\n      <el-col :span=\"12\">\n        <el-card  body-style=\"padding:34px\">\n          <div slot=\"header\" class=\"clearfix\">\n            <span style=\"font-size:25px\">个人第一学期的基本情况</span>\n          </div>\n          <div style=\"font-size:20px\">第一学期综合素质分:{{data.studentCard1.term1_avlscore}}</div>\n          <div style=\"font-size:20px\">第一学期综合素质全院排名:{{data.studentCard1.term1_yranking}}</div>\n          <div style=\"font-size:20px\">第一学期综合素质全班排名:{{data.studentCard1.term1_cranking}}</div>\n        </el-card>\n         <el-card  body-style=\"padding:34px\">\n          <div slot=\"header\" class=\"clearfix\">\n            <span style=\"font-size:25px\">个人第二学期的基本情况</span>\n          </div>\n          <div style=\"font-size:20px\">第二学期综合素质分:{{data.studentCard2.term2_avlscore}}</div>\n          <div style=\"font-size:20px\">第二学期综合素质全院排名:{{data.studentCard2.term2_yranking}}</div>\n          <div style=\"font-size:20px\">第二学期综合素质全班排名:{{data.studentCard2.term2_cranking}}</div>\n        </el-card>\n      </el-col>\n      <!-- 雷达图 -->\n      <el-col :span=\"12\" :xs=\"24\">\n        <el-card>\n        <v-chart class=\"chart\" ref=\"scuhindexscore\" autoresize>\n           <!-- 用ref定义一个变量名字，在下面调用 -->\n        </v-chart>\n        </el-card>\n      </el-col>\n    </el-row>\n    \n    <!-- 第二行 -->\n    <el-row :gutter=\"40\" style=\"margin-top: 25px;\">\n      <!-- 各个指标在全院的排名 -->\n      <el-col :span=\"12\" :xs=\"24\">\n        <el-card >\n        <div style=\"font-size:20px\">各个指标在全院的排名</div>\n        <el-table :data=\"CollegeData\"  fit  align=\"center\">\n          <el-table-column prop=\"Collegindex\" label=\"各指标\" width=\"180\"></el-table-column>\n          <el-table-column prop=\"Collegscores\" label=\"分数\" width=\"180\"></el-table-column>\n          <el-table-column prop=\"Collegranking\" label=\"排名\" width=\"180\"></el-table-column>\n        </el-table>\n        </el-card>\n      </el-col>\n      <!-- 各个指标在全班的排名 -->\n      <el-col :span=\"12\" :xs=\"24\">\n        <el-card >\n         <div style=\"font-size:20px\">各个指标在全班的排名</div>\n        <el-table :data=\"ClassData\"  fit align=\"center\">\n          <el-table-column prop=\"Classindex\" label=\"各指标\" width=\"180\"></el-table-column>\n          <el-table-column prop=\"Classscores\" label=\"分数\" width=\"180\"></el-table-column>\n          <el-table-column prop=\"Classranking\" label=\"排名\" width=\"180\"></el-table-column>\n        </el-table>\n        </el-card>\n      </el-col>\n    </el-row>\n    <!-- 第三行 -->\n    <el-row>\n      <!-- 建议 -->\n      <el-col>\n        <el-collapse v-model=\"activeNames\" @change=\"handleChange\">\n          <el-collapse-item title=\"建议\" name=\"1\">\n            <div>{{proposal}}</div>\n            <div>{{proposal2}}</div>\n          </el-collapse-item>\n        </el-collapse>\n      </el-col>\n    </el-row>\n  </div>\n</template>\n\n<script>\nimport { StudentData } from \"../api/teststudent.js\";\nimport { store } from \"../store.js\";\nimport { EventBus } from \"../event-bus.js\";\n\nexport default {\n  name: \"student\",\n  mounted() {\n    EventBus.$on(\"studentDataLoad\", data => {\n      console.log(\"个人界面请求数据\");\n      console.log(data);\n      this.data = data;\n       //调用定义的变量\n      this.set_suchindexscore();\n      this.hideLoad();\n\n      let score = 0;\n      let indexs=['思想政治','身心健康','创新创业','技术技能','志愿服务','人文艺术','综合素质理论']//用于建议卡片调用\n      //通过判断各指标的循环对比得出最低的指标分数给出建议\n      let minIndex=0\n      let temp=this.data.suchindexscores[2].value;\n      for (let i=0;i<=temp.length-2;i+=1){\n        if (temp[i]<temp[minIndex]){minIndex=i}\n      }\n      this.proposal2=\"你的\"+indexs[minIndex]+\"指标较低，有待加强\"\n\n      if (store.state.term === \"term1\")\n        score = this.data.studentCard1.term1_avlscore;\n      else score = this.data.studentCard2.term2_avlscore;\n      if (score >= 0 && score <= 15) {\n        this.proposal = \"你的综合素质分较低，有待加强\";\n      } else if (score > 15 && score <= 30) {\n        this.proposal = \"你的综合素质分一般，应该适当加强\";\n      } else if (score > 30 && score <= 50) {\n        this.proposal = \"你的综合素质分还不错，继续加油\";\n      } else if (score > 50 && score <= 100) {\n        this.proposal = \"你的综合素质分挺好的\";\n      }\n    });\n    this.showLoad();\n    //请求个人数据\n    EventBus.$emit(\"requestData\", \"个人\");\n  },\n\n  methods: {\n    handleChange(val) {\n      console.log(val);\n    }, //建议\n    showLoad() {\n      Object.keys(this.$refs).forEach(key => {\n        this.$refs[key].showLoading();\n      });\n    },\n    hideLoad() {\n      //遍历所有图表组件实例，调用隐藏加载\n      Object.keys(this.$refs).forEach(key => {\n        this.$refs[key].hideLoading();\n      });\n    },\n    set_suchindexscore() {\n      console.log(\"显示个人当前学期的雷达图\");\n      //循环对比的出指标的最大值，用于定义雷达的最大临界点\n      let suchindexscores = this.data['suchindexscores']\n      let maxvalue = -1;    \n      for(let i =0;i<suchindexscores.length;i+=1)\n      {\n        for(let j =0;j<suchindexscores[i].value.length;j+=1)\n        {\n          if(suchindexscores[i].value[j]>maxvalue)\n          maxvalue=suchindexscores[i].value[j]\n        }\n      }\n\n      let option={\n        title: { text:`${this.stateStore.termName()} 各指标雷达图` },\n        tooltip:{},\n         legend:{data:['学院指标平均分','班级指标平均分','学生指标分数'],right:5},\n        radar:{\n          shape :'circle',\n          axisLine :{\n            \n          },\n          indicator:[\n                   { name: \"思想政治\", max: maxvalue , color:\"black\"},\n            { name: \"身心健康\", max: maxvalue ,color:\"red\"},\n            { name: \"创新创业\", max: maxvalue ,color:\"orange\"},\n            { name: \"技术技能\", max: maxvalue ,color:\"pink\"},\n            { name: \"志愿服务\", max: maxvalue ,color:\"green\"},\n            { name: \"人文艺术\", max: maxvalue ,color:\"blue\"},\n            { name: \"综合素质理论\", max: maxvalue ,color:\"purple\"}\n          ]\n        },\n        series:[\n          { name:'学院指标平均分vs班级指标平均分vs学生指标分数',\n            type:\"radar\",\n            data:suchindexscores\n          }\n\n        ]\n\n      }\n     \n      console.log(option);\n      this.$refs[\"scuhindexscore\"].mergeOptions(option);//调用数据写入雷达图\n    },\n\n    set_CollegeData() {\n      \n      this.CollegeData.splice(0, this.CollegeData.length);\n      this.data[\"data1\"][\"CollegeData\"].forEach(element => {\n        this.CollegeData.push(element);\n      });\n    },//各指标在全院的排名\n    set_ClassData() {\n      \n      this.ClassData.splice(0, this.ClassData.length);\n      this.data[\"data2\"][\"ClassData\"].forEach(element => {\n        this.ClassData.push(element);\n      });\n    }//各指标在全班的排名\n  },\n\n  computed: {\n    CollegeData() {\n      return this.data.data1.CollegeData;\n    },\n    ClassData() {\n      return this.data.data2.ClassData;\n    }\n  },\n  data() {\n    return {\n      proposal: [],\n      proposal2:[],\n      stateStore: store.state,\n      data: StudentData,\n\n     \n      activeNames: [\"1\"]//建议卡片的监听\n    };\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views"}]}